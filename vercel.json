{
  "version": 2,
  "routes": [
    {
      "src": "/app-ads\\.txt",
      "dest": "/static/app-ads.txt"
    },
    {
      "src": "/robots\\.txt",
      "dest": "/static/robots.txt"
    },
    {
      "src": "/(.*)\\.(?:png|jpg|jpeg|heic|pdf|txt)$",
      "dest": "/static/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}